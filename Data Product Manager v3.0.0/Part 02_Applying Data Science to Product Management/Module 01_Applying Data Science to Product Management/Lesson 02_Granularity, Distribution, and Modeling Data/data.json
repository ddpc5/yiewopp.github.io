          "key": "8c052402-a368-481b-ba9d-0c27b0b09327",
          "title": "Glossary",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "8c052402-a368-481b-ba9d-0c27b0b09327",
            "completed_at": null,
            "last_viewed_at": null,
